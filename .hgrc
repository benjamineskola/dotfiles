[alias]
log = log -Gf
l = log
pull = pull --update
up = pull
sh = show

[ui]
username = Benjamin Eskola <ben@eskola.me.uk>
ignore.global = ~/.gitignore.global
logtemplate = "commit {label('yellow',node|short)}:{label('yellow',rev)} {label('red', branches)} {label('red', tags)} \nAuthor: {author}\nDate: {date|date}\n\n\t{desc|strip|fill68|tabindent}\n\n"

[extensions]
hgext.bookmarks =
hggit =
strip =
show = ~/Code/hgext/show.py
