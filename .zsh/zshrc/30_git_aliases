alias git="echo nope; :"

alias g="command git"
alias gadd="command git add"
alias gaddp="command git add -p"
alias gamend="command git commit --amend -CHEAD"
alias gci="command git commit"
alias gd="command git diff -M"
alias gdc="command git diff -M --cached"
alias gl="command git log -M --graph --decorate --abbrev-commit"
alias gnuke="command git reset --hard"
alias gp="command git push"
alias grm="command git rm"
alias gsh="command git show"
alias gst="command git status -sb"
alias gundo="command git checkout HEAD -- "
alias gundop="command git checkout -p HEAD -- "
alias gup="command git up"

compdef _git-branch gbr
