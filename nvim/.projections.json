{
  "plugin/*.vim": { "type": "vimplugin", "alternate": "autoload/{}.vim" },
  "autoload/*.vim": { "type": "vimautoload", "alternate": "plugin/{}.vim" },
  "compiler/*.vim": { "type": "vimcompiler" },
  "ftdetect/*.vim": { "type": "vimftdetect" },
  "syntax/*.vim": {
    "type": "vimsyntax",
    "alternate": ["ftplugin/{}.vim", "indent/{}.vim"]
  },
  "ftplugin/*.vim": {
    "type": "vimftplugin",
    "alternate": ["indent/{}.vim", "syntax/{}.vim"]
  },
  "indent/*.vim": {
    "type": "vimindent",
    "alternate": ["syntax/{}.vim", "ftplugin/{}.vim"]
  },
  "after/*.vim": { "type": "vimafter" },
  "doc/*.txt": { "type": "vimdoc" }
}
